#include <iostream>
#include <vector>
using namespace std;

int firstOccurrence(vector<int>& arr, int key) {
    int left = 0, right = arr.size() - 1;
    int result = -1;
    while(left <= right) {
        int mid = left + (right - left)/2;
        if(arr[mid] == key) {
            result = mid;
            right = mid - 1; // move left to find first occurrence
        } else if(arr[mid] < key) {
            left = mid + 1;
        } else {
            right = mid - 1;
        }
    }
    return result;
}

int main() {
    int n, key;
    cin >> n;
    vector<int> arr(n);
    for(int i=0;i<n;i++) cin >> arr[i];
    cin >> key;
    cout << firstOccurrence(arr, key);
    return 0;
}
