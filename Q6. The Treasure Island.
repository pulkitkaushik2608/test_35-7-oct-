#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;
    vector<vector<int>> grid(n, vector<int>(m));
    for(int i = 0; i < n; i++)
        for(int j = 0; j < m; j++)
            cin >> grid[i][j];
    
    int maxRow = 0;
    int maxSum = 0;
    for(int i = 0; i < n; i++) {
        int sum = 0;
        for(int j = 0; j < m; j++)
            sum += grid[i][j];
        if(sum > maxSum) {
            maxSum = sum;
            maxRow = i;
        }
    }
    cout << "Row " << maxRow + 1 << " (sum=" << maxSum << ")";
    return 0;
}
