#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<vector<int>> arr(n, vector<int>(n));
    for(int i = 0; i < n; i++)
        for(int j = 0; j < n; j++)
            cin >> arr[i][j];

    int mainDiag = 0, antiDiag = 0;
    for(int i = 0; i < n; i++) {
        mainDiag += arr[i][i];
        antiDiag += arr[i][n-1-i];
    }

    cout << "Main diagonal sum = " << mainDiag << ", Anti-diagonal sum = " << antiDiag;
    return 0;
}
