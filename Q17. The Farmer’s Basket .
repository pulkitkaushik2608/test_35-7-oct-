#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, key;
    cin >> n;
    vector<int> arr(n);
    for(int i=0;i<n;i++) cin >> arr[i];
    cin >> key;

    int index = -1;
    for(int i=0;i<n;i++) {
        if(arr[i]==key) {
            index = i;
            break;
        }
    }

    cout << index;
    return 0;
}
