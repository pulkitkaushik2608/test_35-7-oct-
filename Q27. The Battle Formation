#include <iostream>
#include <vector>
using namespace std;

bool searchMatrix(vector<vector<int>>& matrix, int target) {
    int n = matrix.size();
    int m = matrix[0].size();
    int left = 0, right = n*m - 1;

    while(left <= right) {
        int mid = left + (right - left)/2;
        int midVal = matrix[mid/m][mid%m]; // convert 1D index to 2D
        if(midVal == target) return true;
        else if(midVal < target) left = mid + 1;
        else right = mid - 1;
    }
    return false;
}

int main() {
    int n, m, target;
    cin >> n >> m;
    vector<vector<int>> matrix(n, vector<int>(m));
    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
            cin >> matrix[i][j];
    cin >> target;

    cout << (searchMatrix(matrix, target) ? "True" : "False");
    return 0;
}
