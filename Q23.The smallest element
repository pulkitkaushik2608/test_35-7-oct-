#include <iostream>
#include <vector>
using namespace std;

int ceilElement(vector<int>& arr, int target) {
    int left = 0, right = arr.size() - 1;
    int ans = -1;
    while(left <= right) {
        int mid = left + (right - left)/2;
        if(arr[mid] >= target) {
            ans = arr[mid];       // potential ceil
            right = mid - 1;      // move left to find smaller >= target
        } else {
            left = mid + 1;
        }
    }
    return ans;
}

int main() {
    int n, target;
    cin >> n;
    vector<int> arr(n);
    for(int i=0;i<n;i++) cin >> arr[i];
    cin >> target;
    cout << ceilElement(arr, target);
    return 0;
}
